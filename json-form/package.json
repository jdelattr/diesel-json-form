{
  "name": "@diesel-parser/json-form",
  "version": "5.2.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@diesel-parser/json-schema-facade-ts": "^0.13.1",
    "i18next": "^25.6.1",
    "tea-pop-core": "^6.0.1",
    "tea-pop-menu": "^6.0.1"
  },
  "peerDependencies": {
    "@carbon/icons-react": "^11.70.0",
    "@carbon/react": "^1.95.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-tea-cup": "^5.1.1",
    "tea-cup-fp": "^1.1.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "jest": "30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "npm-scripts-watcher": "^1.0.2",
    "rimraf": "^6.1.0",
    "sass": "^1.93.3",
    "ts-jest": "29.4.5"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && npm run build:ts && npm run test && npm run build:sass",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "build:sass-dev": "sass --source-map --load-path=../node_modules/ src/JsonEditor.scss dist/JsonEditor.css",
    "build:sass": "sass --style=compressed --load-path=../node_modules/ src/JsonEditor.scss dist/JsonEditor.css",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "npm-scripts-watcher",
    "prettify": "prettier --write .",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '**/*.{js,ts,tsx}' --fix"
  },
  "watch": {
    "src/**/!(*.test|*.spec).{ts,tsx}": [
      "build:ts"
    ],
    "src/**/*.scss": [
      "build:scss"
    ]
  }
}
